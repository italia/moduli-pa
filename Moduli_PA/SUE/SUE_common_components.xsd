    <!-- Reference suecommon:ConsumiEnergeticiType -->
    <xs:complexType name="ConsumiEnergeticiBaseType" abstract="true"/>

    <xs:complexType name="ConsumiEnergeticiType">
        <xs:complexContent>
            <xs:extension base="suecommon:ConsumiEnergeticiBaseType">
                <xs:sequence>
                    <xs:choice>
                        <xs:element name="nonSoggettoRisparmioEnergetico" minOccurs="0" type="basic:EmptyType">
                            <xs:annotation><xs:documentation>non è soggetto al deposito del progetto e della relazione tecnica di cui all’articolo 125 del 
                                d.P.R. n. 380/2001 e del d.lgs. n. 192/2005</xs:documentation></xs:annotation>
                        </xs:element>                        
                        <xs:element name="soggettoRisparmioEnergetico" minOccurs="0">
                            <xs:annotation><xs:documentation>è soggetto al deposito del progetto e della relazione tecnica di cui 
                                all’articolo 125 del d.P.R. n. 380/2001 e del d.lgs. n. 192/2005</xs:documentation></xs:annotation>
                            <xs:complexType>
                                <xs:complexContent>
                                    <xs:extension base="basic:EmptyType">
                                        <xs:attribute type="xs:boolean" name="allegatoRelazioneTecnica"/>    
                                    </xs:extension>
                                </xs:complexContent>
                            </xs:complexType>
                        </xs:element>                         
                    </xs:choice>
                    <xs:choice>
                        <xs:element name="nonSoggettoFontiRinnovabili" minOccurs="0" type="basic:EmptyType">
                            <xs:annotation><xs:documentation>non è soggetto al deposito del progetto e della relazione tecnica di cui all’articolo 125 del d.P.R. n. 380/2001 e del d.lgs. n. 192/2005
                                che l’intervento, in relazione agli obblighi in materia di fonti rinnovabili</xs:documentation></xs:annotation>
                        </xs:element>                       
                        <xs:element name="soggettoFontiRinnovabili" minOccurs="0">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="prescrizioniInRelTecnica" minOccurs="0" type="basic:EmptyType">
                                        <xs:annotation><xs:documentation>il rispetto delle prescrizioni in materia di utilizzo di fonti di energia rinnovabili è indicato negli elaborati progettuali e 
                                            nella relazione tecnica prevista dall’articolo 125 del d.P.R. n. 380/2001 e dal d.lgs. n. 192/2005 in materia di risparmio energetico</xs:documentation></xs:annotation>
                                    </xs:element>
