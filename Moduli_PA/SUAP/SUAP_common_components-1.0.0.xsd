                        <xs:element name="venditaDirettaUova" type="basic:EmptyType" minOccurs="0" maxOccurs="1"/>
                        <xs:element name="venditaDirettaMiele" type="basic:EmptyType" minOccurs="0" maxOccurs="1"/>
