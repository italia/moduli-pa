                <xs:sequence>
                    <xs:element name="alimentare" minOccurs="0" type="xs:decimal"/>
                    <xs:element name="nonAlimentare" minOccurs="0" type="xs:decimal"/>
                    <xs:element name="complessiva" minOccurs="0" type="xs:decimal"/>
