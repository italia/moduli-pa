                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:complexType name="ClassificazioneBaseType" abstract="true"/>
    <xs:complexType name="ClassificazioneType">
        <xs:complexContent>
            <xs:extension base="suapcommon:ClassificazioneBaseType">
