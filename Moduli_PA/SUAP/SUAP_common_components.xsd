                    <xs:element name="periodo" minOccurs="0" maxOccurs="unbounded" type="aggregate:PeriodoStagionaleType" />
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:complexType name="CarattereTemporaneoBaseType" abstract="true"/>
    <xs:complexType name="CarattereTemporaneoType">
        <xs:complexContent>
            <xs:extension base="suapcommon:CarattereTemporaneoBaseType">
                <xs:sequence>
                    <xs:element name="periodo" minOccurs="0" maxOccurs="unbounded" type="aggregate:PeriodoType" />
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="CarattereEsercizioType">        
