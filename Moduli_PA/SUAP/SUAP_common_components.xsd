                    </xs:sequence>
                </xs:complexType>
            </xs:element>    
        </xs:sequence>        
    </xs:complexType>
        
    <xs:complexType name="OccupazioniMerciIngombrantiNonAlimentariBaseType" abstract="true"/>
    <xs:complexType name="OccupazioniMerciIngombrantiNonAlimentariType">
        <xs:complexContent>
            <xs:extension base="suapcommon:OccupazioniMerciIngombrantiNonAlimentariBaseType">
                <xs:sequence>
                    <xs:element name="merciIngombranti" minOccurs="0" type="xs:decimal"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
